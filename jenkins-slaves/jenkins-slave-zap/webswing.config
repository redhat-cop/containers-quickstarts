{
  "applications" : [ {
    "name" : "ZAP",
    "icon" : "icon.png",
    "mainClass" : "org.zaproxy.zap.ZAP",
    "classPathEntries" : [ "zap-*.jar", "lib/*.jar", "lang/" ],
