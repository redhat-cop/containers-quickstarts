					"kind": "BuildConfig",
					"apiVersion": "v1",
					"metadata": {
						"name": "${HTTPD_APPLICATION_NAME}",
						"labels": {
							"application": "${HTTPD_APPLICATION_NAME}"
						}
					},
					"spec": {
						"triggers": [
							{
								"type": "ConfigChange"
							},
							{
								"type": "ImageChange",
								"imageChange": {}
