						],
						"source": {
							"type": "Git",
							"git": {
								"uri": "${HTTPD_APPLICATION_SOURCE_REPOSITORY_URL}",
								"ref": "${HTTPD_APPLICATION_SOURCE_REPOSITORY_REF}"
