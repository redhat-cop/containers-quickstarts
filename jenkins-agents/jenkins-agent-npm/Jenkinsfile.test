pipeline {
    agent {
      label 'jenkins-agent-npm'
    }

    stages {
        stage ('Run Clone Test') {
            steps {
                sh "git clone https://github.com/redhat-cop/containers-quickstarts.git"
            }
        }

        stage ('Run Test') {
            steps {
              sh """
                  npm -v
                  node -v

                  npm i express
              """
            }
        }
    }
}
